<VBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0" spacing="20.0" style="-fx-background-color: #fafafa;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.Controller">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="500.0">
         <children>
            <Label layoutX="90.0" layoutY="54.0" text="Login" />
            <Label layoutX="90.0" layoutY="99.0" text="Password" />
            <TextField fx:id="loginField" layoutX="199.0" layoutY="50.0" promptText="Login" />
            <Button fx:id="exitButton" layoutX="252.0" layoutY="147.0" mnemonicParsing="false" text="Exit" />
            <Button fx:id="authButton" layoutX="308.0" layoutY="147.0" mnemonicParsing="false" text="Sign In" />
            <PasswordField fx:id="passwordField" layoutX="199.0" layoutY="95.0" promptText="Password" />
            <Label layoutX="183.0" layoutY="4.0" text="Autorization">
               <font>
                  <Font name="Umpush" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
